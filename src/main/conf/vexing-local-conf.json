{
  "external_url": "http://localhost:8080",
  "adapters": {
    "rubicon": {
      "endpoint": "http://fake-bidder:5150/bid",
      "usersync_url": "none",
      "XAPI": {
        "Username": "none",
        "Password": "none"
      }
    }
  },
  "datacache": {
    "type": "filecache",
    "filename": "app-settings.yml"
  },
  "cache": {
    "scheme": "http",
    "host": "cache-service-host",
    "query": "uuid=%PBS_CACHE_UUID%"
  },
  "recaptcha_url": "https://www.google.com/recaptcha/api/siteverify",
  "recaptcha_secret": "secret_value",
  "host_cookie": {
    "opt_out_url": "http://prebid.org/optout",
    "opt_in_url": "http://prebid.org/optin"
  }
}
